{
    "name": "amazon-datazone-cdk-example",
    "version": "0.1.0",
    "bin": {
        "amazon-datazone-cdk-example": "bin/app.ts"
    },
    "license": "MIT-0",
    "scripts": {
        "build": "tsc && tsc --build --clean && npm run lint",
        "watch": "tsc -w",
        "test": "jest",
