    "engines": {
        "node": ">=18"
    },
    "devDependencies": {
        "@types/jest": "^29.5.12",
        "@types/node": "20.11.24",
        "@typescript-eslint/eslint-plugin": "^7.1.1",
        "@typescript-eslint/parser": "7.1.1",
        "aws-cdk": "2.144.0",
        "eslint": "8.57.0",
        "eslint-plugin-import": "^2.29.1",
        "jest": "^29.7.0",
        "ts-jest": "^29.1.2",
        "ts-node": "^10.9.2",
        "typescript": "~5.3.3",
        "csv": "6.3.8",
        "glob": "10.3.10"
    },
    "dependencies": {
        "@types/uuid": "9.0.8",
        "@typescript-eslint/visitor-keys": "7.1.1",
        "aws-cdk-lib": "2.144.0",
        "constructs": "^10.3.0",
        "source-map-support": "^0.5.21"
    }
